<style>

        .alo-phone {
            background-color: transparent;
            bottom: 50%;
            cursor: pointer;
            height: 60px;
            right: 5px;
            position: fixed;
            visibility: visible;
            width: 100px;
            z-index: 9999;
        }

        .alo-phone>span {
            background: var(--second-color) none repeat scroll 0 0;
            border-radius: 5px;
            color: #fff;
            font-weight: bold;
            left: -16px;
            position: absolute;
            top: 85px;
            font-size: 15px;
            padding: 2px 5px;
        }

        .animated {
            -webkit-animation-duration: 1s;
            animation-duration: 1s;
            -webkit-animation-fill-mode: both;
            animation-fill-mode: both;
        }

        .animated.infinite {
            -webkit-animation-iteration-count: infinite;
            animation-iteration-count: infinite;
        }

        .alo-ph-circle {
            background-color: transparent;
            border: 2px solid #f44846 ;
            border-radius: 100%;
            height: 85px;
            left: -7px;
            opacity: 0.5;
            position: absolute;
            top: -7px;
            width: 85px;
        }

        .alo-ph-circle-fill {
            background-color: rgba(244, 72, 70, 0.5);
            border: 2px solid transparent;
            border-radius: 100%;
            height: 70px;
            left: 1px;
            opacity: 0.75;
            position: absolute;
            top: 1px;
            transition: all 0.5s ease 0s;
            width: 70px;
        }

        .alo-ph-img-circle {
            background: #f44846 url("/images/phone.png") no-repeat scroll center center;
            border: 2px solid transparent;
            border-radius: 100%;
            height: 45px;
            left: 13px;
            opacity: 0.7;
            position: absolute;
            top: 13px;
            transform-origin: 50% 50% 0;
            transition: all 0.2s ease-in-out 0s;
            width: 45px;
        }

        .zoomIn {
            -webkit-animation-name: zoomIn;
            animation-name: zoomIn;
        }

        .pulse {
            -webkit-animation-name: pulse;
            animation-name: pulse;
        }
</style>


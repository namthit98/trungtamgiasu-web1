    <title>Trang chủ</title>
    <style>
        :root {
            --main-color: #2E9EE6;
            --main-color-daker: #3793ce;
            --second-color: #ffb100;

            /* --main-color: #ff0000;
            --main-color-daker: #eb4102;
            --second-color: #ff4d00;    */
        }

        .hotline span {
            text-transform: uppercase;
            font-size: 12px;
        }

        .phone {
            color: var(--main-color);
            font-weight: bold;
            font-size: 20px;
        }

        .my-nav.nav-pills .nav-link.active {
            background: var(--main-color);
            color: white !important
        }

        ul.my-nav li:hover {
            background: var(--main-color);
            border-radius: .25rem;
        }

        ul.my-nav li:hover>a {
            color: white !important;
        }

        .button-wrapper {
            background: var(--main-color);
            color: white;
            /* width: 100vw; */
            margin-left: -16px;
            margin-right: -16px;
        }

        .button-wrapper .navbar-toggler {
            outline: none;
            color: white;
            font-size: 14px;
            padding-top: 10px;
            padding-bottom: 10px;
            border: none;
            text-transform: uppercase;
        }

        nav.navbar {
            padding-bottom: 0;
        }

        .banner {
            overflow: hidden;
            padding-right: 0px;
            padding-left: 0px;
        }

        .navbar-collapse {

        <%- include('banner', {categories: categories, parentCategories: parentCategories, active: active}); %>


    <!-- START BANNER -->
    <div class="banner container-fluid">
        <div class="row justify-content-center mt-lg-2">
            <div class="col-12 col-lg-9">
                <div class="pt-3 pb-3" style="background: url('/images/banner.png');background-size: cover;">
                    <h3 class="text-1">LIÊN HỆ TÌM GIA SƯ TẠI NHÀ MIỄN PHÍ !</h3>
                    <h3 class="text-2">MIỄN PHÍ HỌC THỬ 2 BUỔI</h3>
                    <h3 class="text-3">GIA SƯ NAM-NỮ KINH NGHIỆM, HỌC PHÍ CHỈ TỪ 80K - 2 GIỜ TẠI NHÀ</h3>
                    <h3 class="text-4">GỌI NGAY ĐỂ NHẬN TƯ VẤN MIỄN PHÍ</h3>
                </div>
            </div>
        </div>
    </div>
    <!-- END BANNER -->


    <!-- START MAIN -->
    <main>
        <section class="reason p-5">
            <div class="container">
                <div class="row mb-5">
                    <div class="col-12 text-center">
                        <h3><span class="highlight">VÌ SAO</span> BẠN NÊN CHỌN HỌC GIA SƯ DẠY TẠI NHÀ?</h3>
                        <img class="img-fluid"
                            src="http://giasutamduc.net/wp-content/themes/giasuhoangyen/images/arrow.png" alt="">

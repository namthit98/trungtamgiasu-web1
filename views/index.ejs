        }

        .thu-ngo {
            background: var(--main-color);
        }

        .thu-ngo .content {
            color: white;
        }

        .thu-ngo .text {
            border-left: 5px solid var(--second-color);
            padding-left: 20px;
            font-style: italic;
        }

        .thu-ngo .title {
            font-family: 'Dancing Script', cursive;
            font-size: 50px;
        }

        .album .image-card {
            position: relative;
            width: 100%;
            top: 0;
            left: 0;
            -webkit-transition: all 0.3s;
            -moz-transition: all 0.3s;
            -o-transition: all 0.3s;
            transition: all 0.3s;
        }

        .item:hover .album-title {
            top: 0;
            visibility: visible;
            cursor: pointer;
        }

        .album-title {
            position: absolute;
            background: rgba(0, 0, 0, 0.5);
            width: 100%;
            height: 100%;
            top: 100%;
            padding: 0 15px 0 15px;
            left: 0;
            z-index: 1;
            overflow: auto;
            visibility: hidden;
            -webkit-transition: all 0.3s;
            -moz-transition: all 0.3s;
            -o-transition: all 0.3s;
            transition: all 0.3s;
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .album-title h5 a {
            color: #fff;
            text-decoration: none;

        }

        footer {
            background: #0B0900;
            padding: 20px 0;
        }

        footer .footer-title {
            border-bottom: 1px solid black;
            margin-bottom: 10px;
        }

        footer .footer-title h3 {
            border-bottom: 1px solid #fff;
            color: var(--second-color);
            font-family: sfu_eurostilecondensed;
            font-size: 15px;
            margin-top: 0;
            text-transform: uppercase;
            font-weight: bold;
            text-align: center;
            line-height: 30px;
        }

        footer ul {
            list-style: none;
            color: #fff;
            padding: 0;
            font-size: 14px;
        }

        ul#menu-footer-menu li {
            float: left;
            margin: 2px;
        }

        ul#menu-footer-menu li a {
            color: #fff;
            padding: 4px;
            display: block;
            background-color: var(--main-color);
            text-decoration: none;
            transition: .4s;
        }

        ul#menu-footer-menu li a:hover {
            color: var(--main-color);
            background-color: #fff;
        }

        .footer-social {
            display: inline-block;
            padding-top: 10px;
        }

        .footer-social ul {
            list-style: none;
        }

        .footer-social li {
            float: left;
            padding-right: 10px;
        }

        .footer-social li a {
            color: #fff;
        }

        .footer-social .fa {
            font-size: 18px;
            border-radius: 50%;
        }

        .fa-facebook {
            padding: 12px 16px;
            background-color: #2D5F9A;
        }

        .fa-youtube {
            padding: 12px 13px;
            background-color: #cc181e;
        }

        .fa-google-plus {
            padding: 12px 9px;
            background-color: #bd081c;
        }

        .copyright {
            margin-top: 15px;
            color: white;
            font-size: 12px;
        }

        .copyright a {
            color: var(--main-color);
            text-decoration: none;
        }

        .alo-phone {
            background-color: transparent;
            bottom: 28px;
            cursor: pointer;
            height: 60px;
            left: 30px;
            position: fixed;
            visibility: visible;
            width: 60px;
            z-index: 9999;
        }

        .alo-phone>span {
            background: var(--second-color) none repeat scroll 0 0;
            border-radius: 5px;
            color: #fff;
            font-weight: bold;
            left: -20px;
            padding: 3px;
            position: absolute;
            top: 60px;
            font-size: 12px;
        }

        .animated {
            -webkit-animation-duration: 1s;
            animation-duration: 1s;
            -webkit-animation-fill-mode: both;
            animation-fill-mode: both;
        }

        .animated.infinite {
            -webkit-animation-iteration-count: infinite;
            animation-iteration-count: infinite;
        }

        .alo-ph-circle {
            background-color: transparent;
            border: 2px solid #00aff2;
            border-radius: 100%;
            height: 65px;
            left: -7px;
            opacity: 0.5;
            position: absolute;
            top: -7px;
            width: 65px;
        }

        .alo-ph-circle-fill {
            background-color: rgba(0, 175, 242, 0.5);
            border: 2px solid transparent;
            border-radius: 100%;
            height: 50px;
            left: 1px;
            opacity: 0.75;
            position: absolute;
            top: 1px;
            transition: all 0.5s ease 0s;
            width: 50px;
        }

        .alo-ph-img-circle {
            background: #00aff2 url("images/phone.png") no-repeat scroll center center;
            border: 2px solid transparent;
            border-radius: 100%;
            height: 25px;
            left: 13px;
            opacity: 0.7;
            position: absolute;
            top: 13px;
            transform-origin: 50% 50% 0;
            transition: all 0.2s ease-in-out 0s;
            width: 25px;
        }

        .zoomIn {
            -webkit-animation-name: zoomIn;
            animation-name: zoomIn;
        }

        .pulse {
            -webkit-animation-name: pulse;
            animation-name: pulse;
        }

        .tada {
            -webkit-animation-name: tada;
            animation-name: tada;
        }


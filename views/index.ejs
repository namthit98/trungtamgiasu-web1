            justify-content: center;
            align-items: center;
        }

        .one-reason h4 {
            font-size: 15px;
            margin-bottom: 15px;
        }

        .one-reason p {
            text-align: justify;
        }

        .call-to-action a {
            padding: 10px 30px;
            background-color: var(--main-color);
            border-radius: 30px;
            color: #fff;
            font-weight: bold;
            text-transform: uppercase;
            font-size: 20px;
            border: 1px solid var(--main-color);
            box-shadow: 0 5px var(--main-color-daker);
            display: inline-block;
            transition: .4s;
            text-decoration: none;
        }

        .call-to-action a:hover {
            color: var(--main-color);
            background-color: #fff;
            box-shadow: none;
            border: 1px solid var(--main-color);
        }

        .reason.khac-biet {
